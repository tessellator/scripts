#!/usr/bin/env python3

import sys


def spongebobify(text):
    lower = False
    result = []
    for char in text:
        if char.isalpha():
            lower = not lower
            result.append(char.lower() if lower else char.upper())
        else:
            result.append(char)
    return "".join(result)


print(spongebobify(sys.argv[1]), end="")
